{
  "name": "yaml-fragment",
  "version": "1.0.1",
  "description": "A tool to construct a yaml document from smaller yaml documents",
  "main": "src/index",
  "bin": {
    "yaml-fragment": "src/cli.js"
  },
  "keywords": [
    "yaml",
    "openapi",
    "swagger"
  ],
  "author": {
    "name": "Mike Stead",
    "url": "https://github.com/mikestead"
  },
  "license": "MIT",
  "homepage": "https://github.com/mikestead/yaml-fragment",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikestead/yaml-fragment.git"
  },
  "bugs": {
    "url": "https://github.com/mikestead/yaml-fragment/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "mocha --recursive",
    "format": "prettier --write \"{README.md,{src,test}/**/*.*\"}",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "glob": "^6.0.1"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "expect": "^1.13.0",
    "husky": "^0.14.3",
    "mocha": "^2.3.4",
    "prettier": "1.14.0",
    "pretty-quick": "^1.6.0"
  }
}
